  </servlet>
  
  <servlet>
      <servlet-name>EvenementsResponsable</servlet-name>
      <servlet-class>org.ecn.edtemps.servlets.impl.EvenementsResponsableServlet</servlet-class>
  </servlet>
  
  <servlet>
      <servlet-name>GetListeGroupesParentsPotentiels</servlet-name>
      <servlet-class>org.ecn.edtemps.servlets.impl.GroupesParentsPotentielsServlet</servlet-class>
  </servlet>
  
  <servlet>
      <servlet-name>Evenement</servlet-name>
      <servlet-class>org.ecn.edtemps.servlets.impl.EvenementServlet</servlet-class>
  </servlet>

  <servlet>
      <servlet-name>GroupeParticipants</servlet-name>
      <servlet-class>org.ecn.edtemps.servlets.impl.GroupeParticipantsServlet</servlet-class>
  </servlet>

  <servlet>
      <servlet-name>GetListeProprietairesPotentiels</servlet-name>
      <servlet-class>org.ecn.edtemps.servlets.impl.ProprietairesPotentielsServlet</servlet-class>
  </servlet>
	
