      <servlet-class>org.ecn.edtemps.servlets.impl.ParametresCalendrierServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>ListeGroupesUtilisateurProprietaire</servlet-name>
      <servlet-class>org.ecn.edtemps.servlets.impl.GroupesProprietaireServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>RattachementGroupe</servlet-name>
      <servlet-class>org.ecn.edtemps.servlets.impl.RattachementGroupeServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>Disponibilite</servlet-name>
      <servlet-class>org.ecn.edtemps.servlets.impl.DisponibiliteServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>ClientLogging</servlet-name>
    <servlet-class>org.ecn.edtemps.servlets.ClientLoggingServlet</servlet-class>
  </servlet>

  <servlet-mapping>
      <servlet-name>Connection</servlet-name>
      <url-pattern>/identification/connection</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
      <servlet-name>Deconnexion</servlet-name>
      <url-pattern>/identification/disconnect</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
      <servlet-name>CheckConnection</servlet-name>
      <url-pattern>/identification/checkconnection</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
      <servlet-name>Abonnements</servlet-name>
      <url-pattern>/abonnements/*</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
      <servlet-name>GroupesAbonnementsEtNonAbonnements</servlet-name>
      <url-pattern>/abonnementsetnonabonnements</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
      <servlet-name>SAbonner</servlet-name>
      <url-pattern>/sabonner</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
      <servlet-name>SeDesabonner</servlet-name>
      <url-pattern>/sedesabonner</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
      <servlet-name>RecupererMatieresEtTypes</servlet-name>
      <url-pattern>/matieresettypes</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
      <servlet-name>ICalToken</servlet-name>
      <url-pattern>/ical/token</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
      <servlet-name>GetICal</servlet-name>
      <url-pattern>/ical/get</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
      <servlet-name>GetListeMateriels</servlet-name>
      <url-pattern>/listemateriels</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
      <servlet-name>RechercheSalleLibre</servlet-name>
      <url-pattern>/recherchesallelibre</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
      <servlet-name>CalendriersUtilisateur</servlet-name>
      <url-pattern>/mescalendriers</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
      <servlet-name>EvenementsResponsable</servlet-name>
      <url-pattern>/mesevenements</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>GetListeGroupesParentsPotentiels</servlet-name>
    <url-pattern>/groupesparentspotentiels</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
	<servlet-name>Evenement</servlet-name>
	<url-pattern>/evenement/*</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
	<servlet-name>GroupeParticipants</servlet-name>
	<url-pattern>/groupeparticipants/*</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>GetListeProprietairesPotentiels</servlet-name>
    <url-pattern>/proprietairespotentiels</url-pattern>
  </servlet-mapping>
    
  <servlet-mapping>
      <servlet-name>ParametresCalendrier</servlet-name>
      <url-pattern>/calendrier/*</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ListeGroupesUtilisateurProprietaire</servlet-name>
    <url-pattern>/groupesutilisateurproprietaire</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>RattachementGroupe</servlet-name>
    <url-pattern>/rattachementgroupe/*</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
      <servlet-name>Disponibilite</servlet-name>
      <url-pattern>/disponibilitesalle</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
      <servlet-name>ClientLogging</servlet-name>
      <url-pattern>/logging</url-pattern>
  </servlet-mapping>
  

</web-app>

